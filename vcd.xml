<?xml version="1.0" encoding="utf-8"?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="modernizr">
    <CommandPrefix>Modernizr</CommandPrefix>
    <Example>Modernizr search css</Example>
    <Command Name="search">
      <Example>search {message}</Example>
      <ListenFor RequireAppName="BeforeOrAfterPhrase">[please] search {noteSubject}</ListenFor>
      <Feedback>searching Modernizr for {noteSubject}</Feedback>
      <Navigate Target="/search.htm"/>
    </Command>
    <PhraseTopic Label="noteSubject" Scenario="Dictation"></PhraseTopic>
  </CommandSet>
</VoiceCommands>
